package com.pb2{
    public class PB2Wall{

        public var x:Number;
        public var y:Number;
        public var w:Number;
        public var h:Number;
        public var mat:int;

        public function PB2Wall(x:Number, y:Number, w:Number, h:Number, mat:int){
            this.x = x;
            this.y = y;
            this.w = w;
            this.h = h;
            this.mat = mat;
        }
    }
}